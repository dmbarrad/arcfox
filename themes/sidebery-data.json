S */\n\n\n\n/* select what you want */\n\n/* 3 pins per line */\n#root.root {--tabs-pinned-width: calc(100vw / 3 - 9.5px );}\n#root.root {--tabs-pinned-height: 60px;}\n\n/* 4 pins per line *\n#root.root {--tabs-pinned-width: calc(100vw / 4 - 9px );}\n#root.root {--tabs-pinned-height: 50px;}\n\n/* 5 pins per line *\n#root.root {--tabs-pinned-width: calc(100vw / 5 - 8.5px );}\n#root.root {--tabs-pinned-height: 50px;}\n\n\n\n/* audio icon */\n.Tab[data-pin=\"true\"] > .body > .audio { \nbackground: transparent;\nbox-shadow: none;\nright: 3px;\ntop: 3px;\n}\n\n/* bigger favicon */\n.Tab[data-pin=\"true\"] > .body > .fav > .fav-icon { \n  width: 25px;\n  height: 25px;\n  top: -5px;\n  left: -4.5px; \n}\n\n.Tab[data-pin=\"true\"] > .body > .fav > .badge{\n  right: -4.5px;\n  bottom: -4px;\n}\n\n/* shadow, outline and background */\n.Tab[data-pin=\"true\"] .body {\n\tbox-shadow: 0px 0px 9px #00000030;\n  border: 1px solid var(--s-frame-bg);\n  background-color: var(--toolbar-bg)\n}\n\n/* selected outline */\n.Tab[data-pin=\"true\"][data-active=\"true\"] .body \n{border: 1px solid var(--s-frame-fg)}\n\n/* bottom margin */\n.PinnedTabsBar {margin-bottom: 5px}\n#nav_bar {background-color: transparent !important;}\n\n\n\n/* TABS */\n\n/* title text */\n.Tab .title {\n\tbackground: linear-gradient(90deg, var(--tabs-normal-fg) 70%, #f1f1f100 95%);\n\tbackground-clip: text;\n\tcolor: transparent;\n}\n\n\n\n\n/* THIN MODE */\n\n@media (max-width: 51px) {\n\n  /* pinned tabs */\n  #root.root {--tabs-pinned-width: 37px;}\n  #root.root {--tabs-pinned-height: 37px;}\n\n  .Tab[data-pin=\"true\"] > .body > .fav > .fav-icon { \n  width: 20px;\n  height: 20px;\n  top: -3px;\n  left: -2px; \n  }\n\n  .PinnedTabsBar {margin-bottom: 37px}\n  \n  /* tabs */\n  .Tab[data-pin=\"false\"] {\n    padding-left: 0px !important;\n    padding-inline: 3px !important;\n    \n    .audio{\n      position: absolute !important;\n      height: 5px;\n      left: 4.3px;\n      top: 2px;\n    }\n  }\n  \n  .Bot